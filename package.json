{
  "name": "youtube-transcript-downloader",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5"
  },
  "scripts": {
    "start": "set \"NODE_OPTIONS=--openssl-legacy-provider --no-deprecation\" && set \"GENERATE_SOURCEMAP=false\" && react-scripts start",
    "build": "set \"GENERATE_SOURCEMAP=false\" && set \"ESLINT_NO_DEV_ERRORS=true\" && react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test --watchAll=false --coverage --silent",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx --fix --quiet",
    "lint:check": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "clean": "rmdir /s /q build && rmdir /s /q node_modules\\.cache",
    "deploy:build": "npm run clean && npm run build",
    "serve": "npx serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-console": "off",
      "react-hooks/exhaustive-deps": "warn",
      "no-debugger": "warn",
      "default-case": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4",
    "eslint": "^8.30.0",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.4",
    "webpack-bundle-analyzer": "^4.7.0"
  },

  
  "jest": {
    "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/pages/Marketing/**",
    "!src/components/MarketingHeader.js",
    "!src/pages/Marketing/Legal/**"
    ]
  },

  "homepage": ".",
  "proxy": "http://localhost:8000"

}
